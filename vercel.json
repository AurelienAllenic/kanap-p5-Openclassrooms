{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/images/(.*)",
        "dest": "back/images/$1"
      },
      {
        "src": "/(.*\\.(html|css|js|png|jpg|jpeg|svg))",
        "dest": "front/$1"
      },
      {
        "src": "/",
        "dest": "front/html/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "front/html/$1"
      }
    ]
  }